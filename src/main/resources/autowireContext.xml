<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->


    <bean class="autowireXML.Address" id = "address">
        <property name = "houseNo" value = "100"/>
        <property name = "city" value = "Pune"/>
        <property name = "pincode" value = "411036"/>
    </bean>

    <bean class="autowireXML.Student" id="stdId" autowire="byName">
        <property name = "name" value = "Akshay"/>
        <property name = "rollNo" value = "109130"/>
    </bean>
</beans>